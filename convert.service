
[Unit]
Wants=network.target
After=network.target
PartOf=adsb-exchange-978.service
ReloadPropagatedFrom=adsb-exchange-978.service

[Service]
EnvironmentFile=/usr/local/share/adsb-exchange-978/default
EnvironmentFile=/etc/default/adsb-exchange-978
SyslogIdentifier=adsb-exchange-978-convert
User=adsb-exchange-978
ExecStart=-/usr/local/share/adsb-exchange-978/convert.sh
Type=simple
Restart=always
RestartSec=30

[Install]
WantedBy=default.target
